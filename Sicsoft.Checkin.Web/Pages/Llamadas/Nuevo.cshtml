                                <input asp-for="Input.SinRepuestos" class="form-check-input" type="checkbox" id="sinRepuestos">
                                <span class="form-check-sign"></span>
                                Sin Accesorios
                            </label>
                            <label class="form-check-label">
                                <input class="form-check-input" type="checkbox" id="sinFacturar">
                                <span class="form-check-sign"></span>
                                Sin Facturar
                            </label>
                        </div>

                    </div>


                </div>
                <div class="row">
                    <div class="col-sm-6">

                        <div class="form-group">
                            <label>Prioridad:</label>

                            @if (!string.IsNullOrEmpty(Roles.Where(a => a == "70").FirstOrDefault()))
                            {
                                <select id="prioridad" class="form-control" asp-for="Input.Prioridad" required>
                                    <option value="L">Baja</option>
                                    <option value="H">Alta</option>


                                </select>
                            }
                            else
                            {
                                <select id="prioridad" class="form-control" asp-for="Input.Prioridad" required disabled>
                                    <option value="L" selected>Baja</option>

                                </select>
                            }


                        </div>
                    </div>
                </div>

            </div>
        </div>
    </div>
</div>


